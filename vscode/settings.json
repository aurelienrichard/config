{
	"workbench.iconTheme": "catppuccin-perfect-mocha",
	"workbench.tree.indent": 20,
	"workbench.settings.enableNaturalLanguageSearch": false,
	"workbench.enableExperiments": false,
	"workbench.statusBar.visible": false,
	"workbench.editor.showTabs": "multiple",
	"workbench.sideBar.location": "right",
	"workbench.cloudChanges.autoResume": "off",
	"workbench.cloudChanges.continueOn": "off",
	"editor.fontFamily": "'JetBrainsMonoNL Nerd Font', monospace",
	"editor.fontSize": 14,
	"editor.overviewRulerBorder": false,
	"editor.fontWeight": "normal",
	"editor.letterSpacing": 0,
	"editor.fontLigatures": false,
	"editor.rulers": [100],
	"editor.bracketPairColorization.enabled": true,
	"editor.snippetSuggestions": "top",
	"git.autofetch": true,
	"window.customMenuBarAltFocus": false,
	"editor.wordWrap": "wordWrapColumn",
	"files.exclude": {
		"**/node_modules": true
	},
	"files.associations": {
		"*.css": "tailwindcss",
		".env*": "dotenv"
	},
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"svelte.enable-ts-plugin": true,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.fontFamily": "'JetBrainsMonoNL Nerd Font', monospace",
	"terminal.integrated.fontSize": 14,
	"terminal.integrated.letterSpacing": 0,
	"terminal.integrated.tabs.enabled": true,
	"terminal.integrated.defaultProfile.windows": "Debian (WSL)",
	"editor.detectIndentation": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.minimap.enabled": false,
	"breadcrumbs.filePath": "off",
	"security.workspace.trust.untrustedFiles": "open",
	"editor.formatOnSave": true,
	"workbench.editor.labelFormat": "short",
	"editor.lineNumbers": "relative",
	"workbench.panel.defaultLocation": "right",
	"breadcrumbs.enabled": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.insertSpaces": false,
	"liveServer.settings.donotShowInfoMsg": true,
	"editor.wordWrapColumn": 100,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"vscode-neovim.neovimExecutablePaths.linux": "~/nvim.appimage",
	"vscode-neovim.useWSL": true,
	"prettier.arrowParens": "always",
	"prettier.printWidth": 100,
	"prettier.semi": false,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,
	"html.format.wrapLineLength": 100,
	"explorer.compactFolders": false,
	"workbench.editor.enablePreview": false,
	"eslint.validate": ["javascript", "typescript", "svelte"],
	"eslint.probe": ["javascript", "typescript", "svelte"],
	"git.enableCommitSigning": true,
	"workbench.startupEditor": "none",
	"playwright.reuseBrowser": true,
	"workbench.colorTheme": "Catppuccin Mocha",
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"editor.tokenColorCustomizations": {
		"[*Light*]": {
			"textMateRules": [
				{
					"scope": "ref.matchtext",
					"settings": {
						"foreground": "#000"
					}
				}
			]
		},
		"[*Dark*]": {
			"textMateRules": [
				{
					"scope": "ref.matchtext",
					"settings": {
						"foreground": "#fff"
					}
				}
			]
		},
		"textMateRules": []
	},
	"dotenv.enableAutocloaking": false,
	"githubPullRequests.pushBranch": "always",
	"remote.autoForwardPortsSource": "hybrid",
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"github.copilot.editor.enableAutoCompletions": true,
	"window.confirmSaveUntitledWorkspace": false,
	"window.menuBarVisibility": "compact",
	"window.titleBarStyle": "custom"
}
